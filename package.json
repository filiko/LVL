  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cypress": "^13.6.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
